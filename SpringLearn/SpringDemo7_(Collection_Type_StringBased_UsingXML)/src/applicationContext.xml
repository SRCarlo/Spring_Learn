<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="emp" class="Employee" scope="prototype">
		<property name="eid" value="1" />
		<property name="ename" value="Naoe" />
		<property name="Qualification">
			<list>
				<value>B.Tech</value>
				<value>M.Tech</value>
				<value>PHD</value>
				<value>Scientist</value>
				<null />
			</list>
		</property>
		<property name="Skillset">
			<set>
				<value>Java</value>
				<value>Google Cloud</value>
				<value>AWS</value>
				<value>SpringBoot</value>
			</set>
		</property>
		<property name="idproof">
			<map>
				<entry key="101" value="AdharCard" />
				<entry key="102" value="PanCard" />
			</map>
		</property>
	</bean>
</beans>